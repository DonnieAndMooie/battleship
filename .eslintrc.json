{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "quotes": ["error", "double"],
        "semi": ["error", "always"],
        "no-console": "off",
        "no-restricted-syntax": "off",
        "no-param-reassign": "off",
        "no-loop-func": "off"
    }
}
